class Solution{
    public static int doUnion(int a[], int n, int b[], int m) 
    {
        Set<Integer> s1=new HashSet<Integer>();
        Set<Integer> s2=new HashSet<Integer>();
        int i;
        for(i=0;i<n;i++)
        {
            s1.add(a[i]);
        }
         for(i=0;i<m;i++)
        {
            s1.add(b[i]);
        }
        
        return s1.size();
    }
}
