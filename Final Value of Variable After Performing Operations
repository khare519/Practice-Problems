class Solution {
    public int finalValueAfterOperations(String[] operations) {
        int x=0,i;
        for(i=0;i<operations.length;i++)
        {
            String ch=operations[i];
             if(ch.equals("--X")==true||ch.equals("X--")==true)
                 x-=1;
            else 
                x+=1;
        }
        return x;
    }
}
