class Solution {
    public void reverseString(char[] s) {
        int l=0,r=s.length-1,i;
        while(l<r)
        {
            char temp=s[l];
            s[l]=s[r];
            s[r]=temp;
            l++;
            r--;
        }
        
        for(i=0;i<s.length;i++)
            System.out.print(s[i]);
    }
}
